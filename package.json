{
    "name": "lemo-client",
    "namespace": "LemoFoundationLtd",
    "version": "0.9.0",
    "description": "Lemochain JavaScript API, connect to a lemochain node over RPC",
    "main": "dist/lemo-client.cjs.js",
    "browser": "dist/lemo-client-browser.cjs.js",
    "dependencies": {
        "@babel/runtime": "7.1.2",
        "axios": "0.18.0",
        "bignumber.js": "7.2.0",
        "crypto-js": "3.1.4"
    },
    "devDependencies": {
        "@babel/core": "7.1.2",
        "@babel/plugin-transform-runtime": "7.1.0",
        "@babel/preset-env": "7.1.0",
        "babel-eslint": "10.0.1",
        "eslint": "4.12.0",
        "eslint-config-airbnb-base": "12.1.0",
        "eslint-friendly-formatter": "3.0.0",
        "eslint-plugin-import": "2.8.0",
        "mocha": "5.2.0",
        "rollup": "0.66.0",
        "rollup-plugin-babel": "4.0.3",
        "rollup-plugin-commonjs": "9.1.0",
        "rollup-plugin-eslint": "5.0.0",
        "rollup-plugin-json": "3.1.0",
        "rollup-plugin-node-resolve": "3.0.0",
        "rollup-plugin-replace": "2.1.0",
        "rollup-plugin-uglify": "6.0.0"
    },
    "scripts": {
        "build": "rollup -c && rollup -c rollup-umd.config.js",
        "dev": "rollup -c -w",
        "test": "mocha",
        "pretest": "npm run build",
        "lint": "eslint -f node_modules/eslint-friendly-formatter src --fix"
    },
    "files": [
        "dist"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/LemoFoundationLtd/lemo-client.git"
    },
    "keywords": [
        "lemochain",
        "javascript",
        "API"
    ],
    "engines": {
        "node": ">= 6.0.0"
    },
    "license": "LGPL-3.0"
}
